{
  "name": "partialty-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "lint": "next lint",
    "start": "next start",
    "db:dev": "drizzle-kit studio --config db/drizzle.dev.config.ts",
    "db:dev:check": "drizzle-kit check:sqlite --config db/drizzle.dev.config.ts",
    "db:dev:drop": "drizzle-kit drop --config db/drizzle.dev.config.ts",
    "db:dev:generate": "drizzle-kit generate:sqlite --config db/drizzle.dev.config.ts",
    "db:dev:migrate": "cd ./db && bun migrate.dev.ts",
    "db:dev:pull": "drizzle-kit introspect:sqlite --config db/drizzle.dev.config.ts",
    "db:prod": "drizzle-kit studio --config db/drizzle.prod.config.ts",
    "db:prod:drop": "drizzle-kit drop --config db/drizzle.prod.config.ts",
    "db:prod:generate": "drizzle-kit generate:sqlite --config db/drizzle.prod.config.ts",
    "db:prod:migrate": "cd ./db && bun migrate.prod.ts",
    "db:prod:pull": "drizzle-kit introspect:sqlite --config db/drizzle.prod.config.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@next/env": "^14.2.3",
    "@t3-oss/env-nextjs": "^0.10.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "next": "^14.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.32.0"
  }
}
